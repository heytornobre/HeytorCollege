@model IEnumerable<HeytorCollege.ViewModels.BirthdayDateGroup>

@{
    ViewBag.Title = "Student Body Statistics";
}

<h2>Student Body Statistics</h2>

<table>
    <tr>
        <th>
            Birthday
        </th>
        <th>
            Students
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Birthday)
            </td>
            <td>
                @item.StudentCount
            </td>
        </tr>
    }
</table>